#!/usr/bin/env groovy

pipeline {
    agent any
    
    tools {
        maven 'Apache Maven 3.6.3'
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}